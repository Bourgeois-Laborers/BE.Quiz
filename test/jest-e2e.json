{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/cache": "<rootDir>/../libs/cache/src",
    "@app/cache/(.*)": "<rootDir>/../libs/cache/src/$1",
    "@app/gpt": "<rootDir>/../libs/gpt/src",
    "@app/gpt/(.*)": "<rootDir>/../libs/gpt/src/$1",
    "@app/prisma": "<rootDir>/../libs/prisma/src",
    "@app/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1"
  }
}
